{
  "name": "semantic-release-github-pullrequest",
  "description": "semantic-release plugin to create a github pullrequest",
  "version": "0.0.0-development",
  "engines": {
    "node": ">=14"
  },
  "author": "Alexis Saettler <alexis@saettler.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/asbiin/semantic-release-github-pullrequest.git"
  },
  "bugs": {
    "url": "https://github.com/asbiin/semantic-release-github-pullrequest/issues"
  },
  "homepage": "https://github.com/asbiin/semantic-release-github-pullrequest#readme",
  "keywords": [
    "semantic-release",
    "version",
    "github",
    "pullrequest",
    "publish",
    "push"
  ],
  "scripts": {
    "lint": "eslint lib index.js",
    "prettier": "prettier --write --list-different '**/*.js?(on)'",
    "semantic-release": "semantic-release",
    "pretest": "npm run lint",
    "test": "nyc ava -v"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ]
  },
  "files": [
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "dependencies": {
    "@octokit/core": "^5.0.0",
    "@octokit/rest": "^20.0.1",
    "@semantic-release/error": "^3.0.0",
    "@semantic-release/github": "^8.0.2",
    "aggregate-error": "^3.1.0",
    "clear-module": "^4.1.2",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "js-base64": "^3.6.0",
    "lodash": "^4.17.20",
    "octokit-plugin-create-pull-request": "^5.1.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "bottleneck": "^2.19.5",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.0.0",
    "http-proxy-agent": "^5.0.0",
    "https-proxy-agent": "^5.0.0",
    "nock": "^13.0.7",
    "nyc": "^15.1.0",
    "p-retry": "^4.3.0",
    "prettier": "^2.2.1",
    "proxy": "^1.0.2",
    "proxyquire": "^2.1.3",
    "server-destroy": "^1.0.1",
    "sinon": "^12.0.1",
    "url-join": "^4.0.1"
  },
  "peerDependencies": {
    "semantic-release": ">=18.0.0"
  },
  "nyc": {
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "exclude": [
      "semantic-release-github/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "semi": true
  },
  "publishConfig": {
    "access": "public"
  }
}
